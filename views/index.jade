extends layout

block content
	body.mediawiki.ltr.sidedir-ltr.ns-0.ns-subject.page-Main_page.skin-vector.action-view
		#mw-page-base.noprint
		#mw-head-base.noprint
		#content.mw-body
				a#top
				h1#firstHeading.firstHeading
					span(dir="auto")| #{subdomain}pedia
				#bodyContent
					#siteSub From MediaWiki Demo
					#contentSub Nothing
					#jump-to-nav.mw-jump
						| Jump to: navigation
					#mw-content-text.mw-content-ltr
						p Welcome to #{subdomain}pedia, the free wiki about #{subdomain} that anybody can edit.
						p Questions? Email the webmaster.
					span.mw-headline Getting Started
						| Consult the User's Guide for #{title}
		#printfooter
			| Retrieved from blah
		#p-personal
			h5 Personal tools
		#left-navigation
			#p-namespaces.vectorTabs
				h5 Namespaces
				ul
					li#ca-nstab-main.selected Page
					li#ca-talk.new Discussion
			#p-variants.vectorMenu.emptyPortlet
				.menu
					ul
		#right-navigation
			#p-views.vectorTabs
				h5 Views
					ul
						li#ca-view.selected Read
						li#ca-viewsource View source
						li#ca-history.collapsible View history
			#p-cactions.vectorMenu.emptyPortlet
				h5
					span Actions
						.menu
							ul
			#p-search
				h5
					label(for="searchInput") Search
					form#searchform
		#mw-panel.noprint

		#portal.p-navigation
			h5 Navigation
				.body
					ul
						li 
							a Main Page
						li 
							a Community portal
						li 
							a Current events
						li 
							a Recent changes
						li 
							a Random page
						li 
							a Help
		#portal.p-tb
			h5 Toolbox
				.body
					ul
						li
							a#t-whatlinkshere What links here
						li
							a Related changes
						li
							a Special pages
						li
							a Printable version
						li
							a Permanent link

		#footer
			ul#footer-info
				li This page was last modified
				li This page has been accessed many times
			